/dts-v1/;
/plugin/;
/ {
    compatible = "brcm,bcm2835";

	fragment@0 {
		target-path = "/";
		__overlay__ {
            node@0 {
                my_device {
                    compatible = "first_device";
                    status = "okay";
                    name = "Test";
                    my_value = <12>;
                };
            };
        
            node1: node@1 {
                compatible = "second_device";
                status = "okay";
                name = "namdhay";
                value = [0x01, 0x23, 0x45, 0x67];
            };
		};
	};
};
